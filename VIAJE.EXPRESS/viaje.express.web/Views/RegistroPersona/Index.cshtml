        <!-- Main -->
        <section id="main" class="container medium">
            <header>
                <h2>Usuarios</h2>
                <p>Registro de Usuarios</p>
            </header>
            <div class="box">
                <form method="post" action="#">
                    <div class="row gtr-50 gtr-uniform">
                        <div class="col-12">
                            <input type="text" name="Cedula" id="Cedula" value="" placeholder="Ingrese la Cedula" />
                        </div>
                        <br />

                        <div class="col-12">
                            <input type="text" name="Nombre" id="Nombre" placeholder="Ingrese sus Nombres" rows="6" />
                        </div>
                        <br />
                        <div class="col-12">
                            <input type="text" name="Apellidos" id="Apellidos" value="" placeholder="Ingrese sus Apellidos" />
                        </div>
                        <br />
                        <div class="col-12">
                            <label for="start">Fecha de nacimiento:</label>

                            <input type="date" id="start" name="trip-start"
                                   value="2018-07-22"
                                   min="1990-01-01" max="2023-12-31">
                            <br />
                            <br />
                            <div class="col-12">
                                <input type="text" name="Telefono" id="Telefono" value="" placeholder="Telefono" />
                            </div>
                            <br />
                            <div class="col-12">
                                <input type="text" name="Direccion" id="Direccion" value="" placeholder="Ingrese direccion domiciliaria" />
                            </div>
                            <br />
                            <div class="col-12">
                                <input type="text" name="Correo" id="Correo" value="" placeholder="Ingrese Correo" />
                            </div>
                            <br />
                            <div class="col-12">
                                <input type="text" name="Contraseña" id="Contrasenia" value="" placeholder="Ingrese Contraseña" />
                            </div>
                            <br />

                        </div>
                        <div class="col-12">
                            <ul class="actions special">
                                <li><input type="submit" value="Registrar Usuario" onclick="crear_Chofer()" /></li>
                            </ul>
                        </div>
                    </div>
                </form>
